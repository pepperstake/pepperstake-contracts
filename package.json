{
  "name": "@pepperstake/pepperstake-contracts",
  "version": "0.1.0",
  "description": "Contracts for PepperStake Protocol",
  "scripts": {
    "deploy-goerli": "source .env && forge script Deploy --rpc-url $GOERLI_RPC_PROVIDER_URL --broadcast --verify --etherscan-api-key $ETHERSCAN_API_KEY --sender $SENDER_ADDRESS --optimize --optimizer-runs 200 -vvv --private-key $PRIVATE_KEY",
    "deploy-polygon-mumbai": "source .env && forge script Deploy --rpc-url $POLYGON_MUMBAI_RPC_PROVIDER_URL --broadcast --sender $SENDER_ADDRESS --optimize --optimizer-runs 200 -vvv --private-key $PRIVATE_KEY",
    "deploy-optimism-goerli": "source .env && forge script Deploy --rpc-url $OPTIMISM_GOERLI_RPC_PROVIDER_URL --broadcast --sender $SENDER_ADDRESS --optimize --optimizer-runs 200 -vvv --private-key $PRIVATE_KEY"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pepperstake/pepperstake-contracts.git"
  },
  "keywords": [
    "Solidity"
  ],
  "author": "Jonathan Miller",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pepperstake/pepperstake-contracts/issues"
  },
  "homepage": "https://github.com/pepperstake/pepperstake-contracts#readme",
  "dependencies": {
    "@paulrberg/contracts": "^3.7.0"
  }
}
